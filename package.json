{
  "name": "sydney-qt-extension",
  "version": "1.0.0",
  "author": "juzeon",
  "description": "The browser extension of SydneyQt that enables multiple shortcuts, including resolve CAPTCHA automatically etc.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "vue",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{vue,ts,json,css,scss,md}'"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vuetify": "^3.4.9",
    "webext-bridge": "^6.0.1",
    "webext-storage": "^1.2.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.19",
    "@types/chrome": "^0.0.246",
    "@vitejs/plugin-vue": "^4.4.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.18"
  }
}
